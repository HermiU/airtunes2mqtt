{
  "name": "airtunes2mqtt",
  "version": "1.0.3",
  "description": "This is a little daemon that can retrieve an audio stream via TCP socket or from a Alsa Loopback device and stream it  to Airplay/Airtunes compatible receivers. Via MQTT you can control the receivers volume and enable/disable the receivers.",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "airtunes2mqtt": "./index.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "camo-purge; xo",
    "lintfix": "xo --fix"
  },
  "author": "Sebastian 'hobbyquaker' Raff <hq@ccu.io> (https://github.com/hobbyquaker)",
  "license": "MIT",
  "dependencies": {
    "airtunes": "github:microadam/node_airtunes#7e2d5f6b92755dd1908df7e688eb947c15700550",
    "mqtt": "^2.15.0",
    "yalm": "^4.1.0",
    "yargs": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hobbyquaker/airtunes2mqtt"
  },
  "keywords": [
    "airtunes",
    "airplay",
    "mqtt",
    "smarthome",
    "multi",
    "room",
    "audio",
    "mopidy",
    "stream",
    "speaker",
    "music"
  ],
  "bugs": {
    "url": "https://github.com/hobbyquaker/airtunes2mqtt/issues"
  },
  "homepage": "https://github.com/hobbyquaker/airtunes2mqtt",
  "devDependencies": {
    "camo-purge": "latest",
    "xo": "latest"
  },
  "xo": {
    "space": 4
  }
}
